		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^0.22.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"formsnap": "^1.0.1",
		"lucide-svelte": "^0.482.0",
		"postcss": "^8.5.3",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.10",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
